<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="es.udc.psi14.lab01trabazo.ParamActiv">


    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/but_ok"
        android:id="@+id/but_ok"
        android:onClick="onOK"
        android:layout_marginTop="104dp"
        android:layout_below="@+id/textEditText"
        android:layout_centerHorizontal="true" />

    <EditText
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:inputType="text"
        android:id="@+id/textEditText"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/but_to_param_activity"
        android:id="@+id/but_to_param_activity"
        android:onClick="onToParamActiv"
        android:layout_below="@+id/but_ok"
        android:layout_centerHorizontal="true" />

</RelativeLayout>
