@phdthesis{castillobellagonaSistemaImportacionAsistida2016,
  title = {Sistema de Importaci\'on Asistida de Liquidaciones de Aseguradoras M\'edicas Mediante El Uso de {{OCR}}},
  author = {Castillo Bellagona, Miguel {\'A}ngel and Linares Franco, Manuel {\'A}ngel and Rodr{\'i}guez Y{\'a}{\~n}ez, Santiago},
  year = {2016},
  abstract = {Este proyecto forma parte de un proyecto de mayor envergadura denominado SEOGA, el cual consiste en un conjunto de servicios orientados a la optimizaci\'on de la gesti\'on de cl\'inicas y hospitales del sector sanitario privado. Con este proyecto se pretende desarrollar un sistema que permita automatizar la incorporaci\'on de la informaci\'on proveniente de documentos de liquidaci\'on en mal estado a bases de datos de SEOGA, para su posterior explotaci\'on. Los principales objetivos que se pretenden conseguir son: ahorrar el tiempo de consulta de los datos de estos documentos (gracias a su digitalizaci\'on), evitar errores en dichas consultas y permitir nuevas v\'ias de explotaci\'on para dicha informaci\'on. Para realizar este sistema se analizaron los distintos formatos de documentosde liquidaci\'on que llegaban escaneados, por fax o por correo ordinario, determinando si estos pod\'ian incorporarse al sistema de forma autom\'atica utilizando un sistema de OCR. La arquitectura de este proyecto sigue conel desarrollo incremental en la arquitectura basada en N-Capas, orientada al dominio de la soluci\'on de SEOGA. Se prev\'e que este proyecto sufra un considerable n\'umero de modificaciones durante su vida \'util debido a que est\'a situado en un entorno cambiante y vol\'atil. Este tipo de arquitectura minimiza los costes de mantenimiento e incrementa la extensibilidad, cualidades necesarias para un proyecto de estas caracter\'isticas. Para el seguimiento y planificaci\'on del proyecto se utiliz\'o la metodolog\'ia \'agil Scrum, especialmente indicada para proyectos como el planteado. SEOGA. Compa\~n\'ia aseguradora. Acto. Liquidaci\'on.Conciliaci\'on. OmniPage},
  collaborator = {{Universidade da Coru{\~n}a}},
  lccn = {004(043)},
  keywords = {Facultade de Informática,Medicina,Reconocimiento de caracteres (Informática),Sistemas de información,solución de tfg,Trabajos fin de carrera,Universidade da Coruña}
}

