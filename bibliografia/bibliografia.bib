@phdthesis{radioabalFerramentaParaAlimentacion2010,
  title = {Ferramenta Para a Alimentaci\'on Autom\'atica de Sistemas de Informaci\'on a Partir de Formularios En Papel},
  author = {Rad{\'i}o Abal, Santiago and Cotelo Lema, Jos{\'e} Antonio and Saavedra Places, {\'A}ngeles},
  year = {2010},
  abstract = {A pesar de que o uso de dispositivos port\'atiles (como poden ser PDAs, tel\'efonos m\'obiles, etc.) como medio para arecollida de informaci\'on no \'ambito empresarial est\'a proliferando moito nos \'ultimos anos, a\'inda son moitas as empresas que contin\'uan usando formularios en papel para a recollida de informaci\'on en almac\'ens, en obra, etc. Isto xera grandes volumes de formularios con informaci\'on importante para a empresa que debe ser introducida nos seus propios sistemas de informaci\'on (Bases de datos, ERPs, etc) para a s\'ua explotaci\'on. Na maior\'ia dos casos, faisea trav\'es dunha introduci\'on manual que consume una considerable cantidade de tempo e recursos, ademais de serpropenso a erros de transcrici\'on. Por\'en, os anteditos formularios non son de escritura libre, sen\'on que conte\~nenestruturas restrinxidas para a recollida de informaci\'on, como poden ser conxuntos de recadros (ou outras figuras) onde se debuxan marcas para escoller opci\'ons ou para formar unha combinaci\'on de n\'umeros ou caracteres. As estruturas m\'ais complexas son celas onde se escriben letras (unha por cela). Debido a estas limitaci\'ons, estes formularios son susceptibles de seren lidos automaticamente con elevado nivel de \'exito. O obxectivo principal deste proxecto \'e, precisamente, automatizar a extracci\'on de datos estruturados contidos neste tipo de formularios en papel e transferir correctamente a informaci\'on capturada a sistemas corporativos e/ou bases de datos. Como obxectivo secundario, proponse desenvolver una ferramenta para facilitar a definici\'on de novos formularios para a recollida de informaci\'on e tam\'en de formularios preexistentes, de maneira que poidan ser lidosautomaticamente e reco\~necida a informaci\'on que conte\~nen. OCR. OMR. ICR. HWCR. Formulario. Plantilla. Imaxe. Esc\'aner},
  collaborator = {{Universidade da Coru{\~n}a}},
  lccn = {004(043)},
  keywords = {Facultade de Informática,Trabajos fin de carrera,Universidade da Coruña}
}

