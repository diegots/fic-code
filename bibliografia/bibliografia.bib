	abstract = {A pesar de que o uso de dispositivos portátiles (como poden ser PDAs, teléfonos móbiles, etc.) como medio para arecollida de información no ámbito empresarial está proliferando moito nos últimos anos, aínda son moitas as empresas que continúan usando formularios en papel para a recollida de información en almacéns, en obra, etc. Isto xera grandes volumes de formularios con información importante para a empresa que debe ser introducida nos seus propios sistemas de información (Bases de datos, ERPs, etc) para a súa explotación. Na maioría dos casos, faisea través dunha introdución manual que consume una considerable cantidade de tempo e recursos, ademais de serpropenso a erros de transcrición. Porén, os anteditos formularios non son de escritura libre, senón que conteñenestruturas restrinxidas para a recollida de información, como poden ser conxuntos de recadros (ou outras figuras) onde se debuxan marcas para escoller opcións ou para formar unha combinación de números ou caracteres. As estruturas máis complexas son celas onde se escriben letras (unha por cela). Debido a estas limitacións, estes formularios son susceptibles de seren lidos automaticamente con elevado nivel de éxito. O obxectivo principal deste proxecto é, precisamente, automatizar a extracción de datos estruturados contidos neste tipo de formularios en papel e transferir correctamente a información capturada a sistemas corporativos e/ou bases de datos. Como obxectivo secundario, proponse desenvolver una ferramenta para facilitar a definición de novos formularios para a recollida de información e tamén de formularios preexistentes, de maneira que poidan ser lidosautomaticamente e recoñecida a información que conteñen. OCR. OMR. ICR. HWCR. Formulario. Plantilla. Imaxe. Escáner},
	author = {Radío Abal, Santiago and Cotelo Lema, José Antonio and Saavedra Places, Ángeles},
	collaborator = {Universidade da Coruña},
	year = {2010},
	keywords = {Facultade de Informática, Trabajos fin de carrera, Universidade da Coruña},
	annote = {Proyecto de Ingeniería Informática Curso 2009/2010},
	annote = {Trabajo fin de carrera-Universidade da Coruña},
}

@phdthesis{castillo_bellagona_sistema_2016,
	title = {Sistema de importación asistida de liquidaciones de aseguradoras médicas mediante el uso de {OCR}},
	abstract = {Este proyecto forma parte de un proyecto de mayor envergadura denominado SEOGA, el cual consiste en un conjunto de servicios orientados a la optimización de la gestión de clínicas y hospitales del sector sanitario privado. Con este proyecto se pretende desarrollar un sistema que permita automatizar la incorporación de la información proveniente de documentos de liquidación en mal estado a bases de datos de SEOGA, para su posterior explotación. Los principales objetivos que se pretenden conseguir son: ahorrar el tiempo de consulta de los datos de estos documentos (gracias a su digitalización), evitar errores en dichas consultas y permitir nuevas vías de explotación para dicha información. Para realizar este sistema se analizaron los distintos formatos de documentosde liquidación que llegaban escaneados, por fax o por correo ordinario, determinando si estos podían incorporarse al sistema de forma automática utilizando un sistema de OCR. La arquitectura de este proyecto sigue conel desarrollo incremental en la arquitectura basada en N-Capas, orientada al dominio de la solución de SEOGA. Se prevé que este proyecto sufra un considerable número de modificaciones durante su vida útil debido a que está situado en un entorno cambiante y volátil. Este tipo de arquitectura minimiza los costes de mantenimiento e incrementa la extensibilidad, cualidades necesarias para un proyecto de estas características. Para el seguimiento y planificación del proyecto se utilizó la metodología ágil Scrum, especialmente indicada para proyectos como el planteado. SEOGA. Compañía aseguradora. Acto. Liquidación.Conciliación. OmniPage},
	author = {Castillo Bellagona, Miguel Ángel and Linares Franco, Manuel Ángel and Rodríguez Yáñez, Santiago},
	collaborator = {Universidade da Coruña},
	year = {2016},
	keywords = {Facultade de Informática, Trabajos fin de carrera, Universidade da Coruña, Medicina, Reconocimiento de caracteres (Informática), Sistemas de información},
	annote = {Curso 2015/2016 Proyecto de Enxeñería Informática},
	annote = {Trabajo fin de carrera-Universidade da Coruña},
}

